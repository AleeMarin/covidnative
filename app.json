{
  "expo": {
    "name": "covidnative",
    "slug": "covidnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aleemarin.covidnative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Mobile application developed in React Native (with Expo) for the visualisation of data from the COVID19 public API https://covid19api.com/",
    "githubUrl": "https://github.com/AleeMarin/covidnative"
  }
}
